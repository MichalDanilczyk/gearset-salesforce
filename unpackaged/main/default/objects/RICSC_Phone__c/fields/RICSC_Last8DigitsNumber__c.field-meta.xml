<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
        <fullName>RICSC_Last8DigitsNumber__c</fullName>
        <description>A formula field which converts the number field to an 8 digit string</description>
        <externalId>false</externalId>
        <formula>RIGHT(SUBSTITUTE(RICSC_Phone__c, "-", ""), 8)</formula>
        <label>Last 8 digits number</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </CustomField>
